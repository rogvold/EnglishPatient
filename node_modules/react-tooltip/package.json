    "classnames": "^1.2.0"
  },
  "devDependencies": {
    "babel": "^5.3.1",
    "babel-eslint": "^4.1.1",
    "babelify": "^6.0.2",
    "http-server": "^0.8.0",
    "node-sass": "^3.3.2",
    "snazzy": "^2.0.1",
    "standard": "^5.2.2",
    "tape": "^4.2.0",
