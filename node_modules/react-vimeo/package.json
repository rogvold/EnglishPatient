{
  "name": "react-vimeo",
  "version": "0.0.3",
  "description": "React component to load video from Vimeo",
  "author": {
    "name": "Berkeley Martinez",
    "email": "berkeley@r3dm.com",
    "url": "https://github.com/berkeleytrue"
  },
  "bugs": {
    "url": "https://github.com/freecodecamp/react-vimeo/issues"
